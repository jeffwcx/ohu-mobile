{
  "name": "ohu-mobile",
  "version": "1.0.0",
  "description": "Fantastic UI Toolkit",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "start": "lerna run start --scope=storybook",
    "build:components": "lerna run build:components --scope=@ohu-mobile/core",
    "build:icons": "lerna run build --scope=@ohu-mobile/icons",
    "create-icons": "lerna run g --scope=@ohu-mobile/icons",
    "storybook": "lerna run build --scope=storybook",
    "test": "lerna run test --scope=@ohu-mobile/core",
    "coverage": "lerna run test:cov --scope=@ohu-mobile/core"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffwcx/ohu-mobile.git"
  },
  "keywords": [
    "components",
    "vue",
    "vue-components",
    "ui-kit",
    "ohu"
  ],
  "author": "jeffwcx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeffwcx/ohu-mobile/issues"
  },
  "homepage": "https://github.com/jeffwcx/ohu-mobile#readme",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-optional-chaining": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@types/node": "^12.12.22",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "@vue/babel-preset-jsx": "^1.1.1",
    "autoprefixer": "^9.7.3",
    "babel-plugin-import": "^1.13.0",
    "conventional-changelog": "^3.1.10",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "lerna": "^3.20.2",
    "typescript": "^3.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
