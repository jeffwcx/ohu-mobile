{
  "compilerOptions": {
    "module": "esnext",
    "declaration": true,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "sourceMap": false,
    "declarationDir": "typings"
  },
  "include": ["icons/**/*.ts"],
  "exclude": ["node_modules"]
}
