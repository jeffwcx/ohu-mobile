{
  "name": "@ohu/cli",
  "version": "0.3.0",
  "description": "cli for ohu",
  "keywords": [
    "cli",
    "ohu"
  ],
  "author": "jeffwcx <jeffwcx@icloud.com>",
  "homepage": "https://github.com/jeffwcx/ohu-mobile/tree/master/packages/ohu-cli#readme",
  "license": "MIT",
  "type": "module",
  "bin": {
    "ohu": "lib/cli.js"
  },
  "main": "lib/index.js",
  "exports": {
    ".": {
      "node": "./lib/index.cjs",
      "import": "./lib/index.js"
    }
  },
  "files": [
    "bin",
    "lib",
    "tests",
    "templates"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffwcx/ohu-mobile.git"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/cli.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "format": [
      "esm",
      "cjs"
    ],
    "outDir": "lib",
    "dts": true
  },
  "scripts": {
    "prepublish": "pnpm typecheck && pnpm test && pnpm build",
    "typecheck": "tsc --noEmit",
    "build": "tsup",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "ohu": "pnpm build && node lib/cli.js"
  },
  "bugs": {
    "url": "https://github.com/jeffwcx/ohu-mobile/issues"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/yargs": "^17.0.32",
    "art-template": "^4.13.2",
    "chalk": "^5.3.0",
    "cosmiconfig": "^9.0.0",
    "ejs": "^3.1.10",
    "fs-extra": "^11.2.0",
    "glob": "^10.4.5",
    "inquirer": "^9.2.23",
    "joi": "^17.12.3",
    "lodash-es": "^4.17.21",
    "ora": "^8.0.1",
    "rimraf": "^5.0.9",
    "svgo": "^3.2.0",
    "tslib": "^2.6.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/mock-fs": "^4.13.4",
    "@types/react": "^18.2.79",
    "mock-fs": "^5.2.0",
    "pkg-dir": "^8.0.0",
    "react": "^18.2.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vue": "^2.7.16"
  }
}
